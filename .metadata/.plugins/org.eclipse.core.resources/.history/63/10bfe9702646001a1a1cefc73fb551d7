package com.cos.springboot.repository;

import java.util.List;

import com.cos.springboot.model.Mem;

// MapperScan에 의해서 메모리에 로드됨.
public interface MemRepository {
	// 전체보기
	List<Mem> findAll();
	// 상세보기
	Mem findById(int id);
	// 회원가입
	void save(Mem mem);
	// 회원수정
	void update(Mem mem);
	// 회원삭제
	void delete(int id);
}
