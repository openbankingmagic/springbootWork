package com.cos.springboot.controller;

import org.springframework.stereotype.Controller;
import org.springframework.web.bind.annotation.GetMapping;


@Controller
public class HomeController {

	// localhost:8080/
	// localhost:8080
	@GetMapping({"","/"})
	public String Home() {
		return "home";
	}
	

		@GetMapping("/hello")
		public String hello() {
			return "hello";
		}
}
